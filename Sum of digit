package Testonline6;

import java.util.Scanner;

public class sumofdigit {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number");
		int y=sc.nextInt();
		int n=0,sum=0;
		while(y>0) {
			System.out.println("Y--->"+y);
			n=y%10;
			sum=n+sum;
			System.out.println(sum);
		
		y=y/10;
		System.out.println();
	}
	}
}
